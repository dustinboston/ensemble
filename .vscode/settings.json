{
  "[json]": { "editor.defaultFormatter": "denoland.vscode-deno" },
  "[jsonc]": { "editor.defaultFormatter": "denoland.vscode-deno" },
  "[typescript]": { "editor.defaultFormatter": "denoland.vscode-deno" },
  "deno.config": "./deno.jsonc",
  "deno.enable": true,
  "deno.lint": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "editor.rulers": [160],
  "editor.tabSize": 2,
  "editor.useTabStops": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "cSpell.words": [
    "Berners",
    "creationflags",
    "Eich",
    "INPUTRC",
    "killpg",
    "preexec",
    "setsid",
    "TIOCSWINSZ"
  ],
  "[clojure]": {
    "editor.defaultFormatter": "pedrorgirardi.vscode-cljfmt"
  }
  // "editor.detectIndentation": false,
  // "[clojure]": {
  //   "editor.defaultFormatter": "betterthantomorrow.calva"
  // },
  // "calva.autoOpenInspector": false,
  // "calva.autoOpenResultOutputDestination": false,
  // "calva.autoSelectNReplPortFromPortFile": false
}
